# Backend (Render)
# Required
PORT=3000
NODE_ENV=production

# Allow your frontend domains (comma-separated)
CORS_ORIGIN=http://localhost:5173,https://your-frontend.vercel.app

# Auth (JWT)
ACCESS_TOKEN_SECRET=change_me_to_a_long_random_secret
ACCESS_TOKEN_EXPIRY=1d

# Database
MONGO_URI_ATLAS=mongodb+srv://<user>:<pass>@<cluster>/<db>?retryWrites=true&w=majority

# Telegram reminders (optional but used by telegram.service)
TELEGRAM_BOT_TOKEN=

# Disease prediction service (Render URL for the disease_model service)
# Example: https://caresentry-disease.onrender.com
DISEASE_MODEL_BASE=
